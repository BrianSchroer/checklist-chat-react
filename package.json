{
    "name": "checklist-chat",
    "version": "1.0.0",
    "description": "SignalR chat with task checklist",
    "config": {
        "port": "3000",
        "mock_api_port": "3001",
        "storybook_port": "6006"
    },
    "scripts": {
        "prestart": "babel-node tools/startMessage.js",
        "start": "npm-run-all --parallel start-mockapi open:src lint:watch",
        "open:src": "babel-node tools/srcServer.js",
        "lint": "esw src tools",
        "lint:watch": "npm run lint -- --watch",
        "localtunnel": "babel-node tools/localtunnel.js",
        "localtunnel-jsonServer": "babel-node tools/localtunnel.jsonServer.js",
        "test": "jest",
        "test:watch": "npm test -- --watch --notify",
        "test-verbose": "npm test -- --verbose",
        "test-verbose:watch": "npm run test:watch -- --verbose",
        "test-coverage": "npm test -- --coverage",
        "generate-mock-data": "babel-node tools/mock/generateMockData.js",
        "prestart-mockapi": "npm run generate-mock-data",
        "start-mockapi": "cross-var json-server --watch tools/mock/mockData.json --port $npm_package_config_mock_api_port",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "build:html": "babel-node tools/buildHtml.js",
        "prebuild": "npm-run-all clean-dist test lint build:html",
        "build": "babel-node tools/build.js",
        "postbuild": "babel-node tools/distServer.js",
        "storybook": "cross-var start-storybook --port $npm_package_config_storybook_port --config-dir ./tools/storybook",
        "build-storybook": "build-storybook --config-dir ./tools/storybook -o .out",
        "deploy-storybook": "storybook-to-ghpages",
        "cypress:open": "cypress open"
    },
    "author": "Brian Schroer",
    "license": "MIT",
    "dependencies": {
        "@kadira/storybook-deployer": "^1.2.0",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^3.3.6",
        "raf": "^3.4.0",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-markdown": "^2.5.0",
        "react-redux": "^5.0.5",
        "react-router-dom": "^4.1.2",
        "redux": "^3.6.0",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "@storybook/addon-actions": "^3.2.12",
        "@storybook/addon-info": "^3.2.12",
        "@storybook/addon-links": "^3.2.12",
        "@storybook/react": "^3.2.12",
        "babel-cli": "^6.8.0",
        "babel-core": "^6.8.0",
        "babel-eslint": "^7.1.1",
        "babel-jest": "^21.2.0",
        "babel-loader": "^6.2.4",
        "babel-plugin-react-display-name": "^2.0.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.6.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-register": "^6.8.0",
        "colors": "^1.1.2",
        "compression": "^1.6.1",
        "cross-var": "^1.0.2",
        "css-loader": "^0.23.1",
        "cypress": "^1.1.0",
        "deep-freeze": "0.0.1",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.1",
        "eslint": "^2.9.0",
        "eslint-plugin-import": "^1.6.1",
        "eslint-plugin-react": "^5.0.1",
        "eslint-watch": "^2.1.11",
        "eventsource-polyfill": "^0.9.6",
        "express": "^4.13.4",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.8.5",
        "jest": "^21.2.1",
        "jest-cli": "^21.2.1",
        "jsdom": "^8.5.0",
        "json-loader": "^0.5.4",
        "json-schema-faker": "^0.4.0",
        "json-server": "^0.9.4",
        "localtunnel": "^1.8.2",
        "mocha": "^2.4.5",
        "nock": "^8.0.0",
        "npm-run-all": "^1.8.0",
        "open": "^0.0.5",
        "prop-types": "^15.6.0",
        "react-addons-test-utils": "^15.6.2",
        "react-jest-snapshot-helper": "^1.0.0",
        "react-test-renderer": "^16.0.0",
        "redux-immutable-state-invariant": "^1.2.3",
        "redux-mock-store": "^1.0.2",
        "rimraf": "^2.5.2",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": "^1.13.0",
        "webpack-dev-middleware": "^1.6.1",
        "webpack-hot-middleware": "^2.10.0"
    },
    "jest": {
        "setupFiles": [
            "raf/polyfill"
        ],
        "testMatch": [
            "**/?(*.)test.js"
        ],
        "testPathIgnorePatterns": [
            "test.integration.js"
        ],
        "collectCoverage": true,
        "coverageReporters": [
            "html"
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/BrianSchroer/checklist-chat-react"
    }
}
