{
  "name": "checklist-chat",
  "version": "1.0.0",
  "description": "SignalR chat with task checklist",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel start-mockapi test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "nodemon --verbose --exec \"npm run lint\"",
    "localtunnel": "babel-node tools/localtunnel.js",
    "localtunnel-jsonServer": "babel-node tools/localtunnel.jsonServer.js",
    "test": "mocha --reporter min tools/testSetup.js \"src/**/*.test.js\" \"tools/**/*.test.js\"",
    "test-debug": "mocha --reporter min tools/testSetup.js \"src/**/*.test.js\" \"tools/**/*.test.js\" --debug-brk",
    "test:watch": "nodemon --verbose --exec \"npm test\"",
    "test-verbose": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\" \"tools/**/*.test.js\"",
    "test-verbose-debug": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\" \"tools/**/*.test.js\" --debug-brk",
    "test-verbose:watch": "nodemon --verbose --exec \"npm run test-verbose\"",
    "test-coverage": "nyc mocha --reporter min tools/testSetup.js \"src/**/*.test.js\" \"tools/**/*.test.js\"",
    "generate-mock-data": "babel-node tools/mock/generateMockData.js",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch tools/mock/mockData.json --port 3001",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js",
    "storybook": "start-storybook --port 6006 --config-dir ./tools/storybook --static-dir ./node_modules/bootstrap/fonts,./node_modules/bootstrap/dist/css,./src/styles,./src/images",
    "build-storybook": "build-storybook --config-dir ./tools/storybook --static-dir ./node_modules/bootstrap/fonts,./node_modules/bootstrap/dist/css,./src/styles,./src/images -o .out",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "author": "Brian Schroer",
  "license": "MIT",
  "dependencies": {
    "@kadira/storybook-deployer": "^1.2.0",
    "babel-polyfill": "^6.8.0",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.7.0",
    "jquery": "^2.2.3",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.8.0",
    "colors": "^1.1.2",
    "compression": "^1.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "^5.0.1",
    "eslint-watch": "^2.1.11",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.19.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jsdom": "^8.5.0",
    "json-schema-faker": "^0.4.0",
    "json-server": "^0.9.4",
    "localtunnel": "^1.8.2",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^1.8.0",
    "open": "^0.0.5",
    "react-addons-test-utils": "^15.0.2",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-mock-store": "^1.0.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BrianSchroer/super-broccoli"
  }
}
